#awk命令
#分割符号
#-F
#以逗号分隔，默认是空格，下标从1开始，0打印整行
cat xx.file｜awk -F',' '{print $2}'
#累加
cat xx.file ｜ awk '{print $12}'| awk '{sum += $1} END {print sum}'
##平均值
cat xx.file ｜ awk '{print $12}'| awk '{sum += $1} END {print sum/NR}'
## 以第一列和第四列为key去重
cat xx.file | awk '!a[$1" "$4]++{print}'
# 输出最后一列
awk '{print $NR}'
